<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

        <hibernate-mapping package="com.model">
        
        <class name="Post" table="post">
        
        
        <id column="pid" name="pid">
        <generator class="increment"></generator>
        </id>
        <property name="id" column="id"></property>
        <property name="definition" column="definition"></property>
        <property name="title" column="title"></property>
        <property name="keyword" column="keyword"></property>
        <property name="abstrct" column="abstrct"></property>
        <property name="description" column="description"></property>
        <property name="category" column="category"></property>
        <property name="status" column="status"></property>
   	  	<property name="startdate" column="startdate"></property>
   	 
   	 </class>        
        
        
        </hibernate-mapping>